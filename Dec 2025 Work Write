SELECT 
        e.id AS emp_id,
        e.sevarth_number,
        e.emp_firstname,
        e.emp_lastname
    FROM emp_master as e
    INNER JOIN employee_profiles as ep ON e.id = ep.emp_id
    INNER JOIN "public"."user" as u ON u.username = e.sevarth_number
    WHERE 
        ep.role_code = 'PROF337_ROLE437'
        AND e.active_status = 1
        AND u.username = 'KMADMIN0001'
        AND u.password = 'e6e061838856bf47e1de730719fb2609'



