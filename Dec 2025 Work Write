SELECT DISTINCT acd.id AS fk_accused_id, ad.major_head_id, ad.fir_no, ad.section, ad.date_of_crime, mjm.english_description AS major, ad.minor_head_id, mim.english_description AS minor, acd.login_sub_unit, um.unit_name AS unitname FROM accused_details AS acd LEFT JOIN ( SELECT ad1.fk_accused_id, ad1.major_head_id, ad1.fir_no, ad1.date_of_crime, ad1.minor_head_id, ad1.police_station, ad1.section FROM accused_crime_dtls AS ad1 INNER JOIN ( SELECT fk_accused_id, MAX(date_of_crime) AS latest_date FROM accused_crime_dtls GROUP BY fk_accused_id ) AS latest ON ad1.fk_accused_id = latest.fk_accused_id AND ad1.date_of_crime = latest.latest_date ) AS ad ON ad.fk_accused_id = acd.id LEFT JOIN major_head_master AS mjm ON mjm.id = ad.major_head_id LEFT JOIN minor_head_master AS mim ON mim.id = ad.minor_head_id LEFT JOIN unit_master AS um ON um.id = ad.police_station WHERE acd.id IN (726,727,728,816,817,818,822,848,980,1307,1472,1515,1516,1521,1522,1526,1529,1530,1531,1532,1545,1633,1635,1643,1644,1802,1815,1821,1823,1826,1831,1920,2004,2008,2345,2426,2464,3508,3548,3590,3702,3748,3750,3790,4093,4173,4446,4571,4576,4581,4736,4943,4949,4984,4985,4986,5004,5490,5543,5562,5646,5724,5760,5976,5979,6003,6006,6047,6051,6054,6057,6059,6448,6604,6862,6982,6996,7103,7106,7108,7208,7209,7710,7726,7925,7996,7999,8153,8156,8264,8283,8285,8286,8289,8291,8390,8395,8447,8606,8611,8718,8746,8922,8926,8929,9098,9100,9102,9104,9106,9109,9114,9122,9123,9126,9127,9129,9132,9137,9153,9156,9159,9163,9164,9168,9171,9313,9424,9694,9796,9826,9847,9936,9944,9958,10180,10240,10241,10354,10383,10433,10530,10564,10788,10790,10792,10820,10862,10864,10878,10895,10909,10958,11150,11185,11260,11310,11652,11657,11743,11771,11797,11885,12005,12052,12206,12207,12477,12479,13157,13199,13236,14431,14432,14449)

