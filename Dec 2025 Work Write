  unit_master_id IN (
    WITH RECURSIVE units AS (
        SELECT um.unit_master_id
        FROM unit_master um
        JOIN user_unit_map uum
            ON uum.unit_master_id = um.unit_master_id
        WHERE uum.sevarth_number = '{{ current_username() }}'

        UNION ALL

        SELECT child.unit_master_id
        FROM unit_master child
        JOIN units parent
            ON child.parent_unit_id = parent.unit_master_id
    )
    SELECT unit_master_id FROM units
)
