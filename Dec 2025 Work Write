SELECT * FROM "public"."unit_master" WHERE parent_cd = 199072241;

WITH RECURSIVE unit_tree AS (
    SELECT id
    FROM unit_master
    WHERE id = 199072241   -- example: ACP PANVEL
 
    UNION ALL
 
    SELECT um.id
    FROM unit_master um
    JOIN unit_tree ut
      ON um.parent_cd = ut.id
)
SELECT ut.id
FROM unit_tree ut
LEFT JOIN unit_master child
  ON child.parent_cd = ut.id
WHERE child.id IS NULL;

