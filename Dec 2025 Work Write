        "SELECT  max(id) as wit_id,  max(id) as id, fg.summons_person_mobile as witness_mobile,
                                fg.summons_person_name as witness_name,fg.summons_person_address
                                from fb_gb_summons fg
                                WHERE fg.summons_witness_id= 150723 group by 3,4,5 "


SELECT 
    fg.id AS wit_id,
    fg.id,
    fg.summons_person_mobile AS witness_mobile,
    fg.summons_person_name AS witness_name,
    fg.summons_person_address
FROM fb_gb_summons fg
WHERE fg.summons_witness_id = 150723
ORDER BY fg.id DESC
LIMIT 1



