WITH RECURSIVE unit_tree AS (
    SELECT id
    FROM unit_master
    WHERE id =  '{{ current_username() }}'

    UNION ALL

    SELECT um.id
    FROM unit_master um
    JOIN unit_tree ut
      ON um.parent_cd = ut.id
),
leaf_units AS (
    SELECT ut.id
    FROM unit_tree ut
    LEFT JOIN unit_master child
      ON child.parent_cd = ut.id
    WHERE child.id IS NULL
)
SELECT v.*
FROM vw_ds_ba_trxn v
JOIN leaf_units lu
  ON v.unit_master_id = lu.id;