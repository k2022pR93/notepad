SELECT 
        e.id AS emp_id,
        e.sevarth_number,
        e.emp_firstname,
        e.emp_lastname,
        d.designation_name
    FROM emp_master e
    INNER JOIN designation_master d ON e.designation_master_id = d.id
    INNER JOIN user u ON u.username = e.sevarth_number
    WHERE 
        e.designation_master_id = 31
        AND e.active_status = 1
        AND u.username = 'NMADMIN0001'
        AND u.password = 'e6e061838856bf47e1de730719fb2609'

