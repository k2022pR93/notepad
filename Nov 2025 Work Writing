SELECT emp_master.emp_firstname ||' '||emp_master.emp_middlename ||' '||emp_master.emp_lastname as io_fname,ems_crime_master.case_number as cr_no,panchanama_date,evidence_details.*,ems_evidence_types.name,designation_master.designation_name as designation,
        	unit_master.unit_name as posting from evidence_details
	        inner join ems_evidence_types on ems_evidence_types.id=evidence_type
	        inner join designation_master on designation_master.desig_master_id=io_designation
	        inner join unit_master on unit_master.id=io_ps
	        inner join ems_crime_master on ems_crime_master.id=evidence_details.fk_ems_case_id
	        inner join ems_panchanama on ems_panchanama.id=fk_ems_panchnama_id
	       -- inner join emp_master on emp_master.id=cast(evidence_details.io_name as integer)
            LEFT JOIN emp_master ON emp_master.id = NULLIF(regexp_replace(evidence_details.io_name, '[^0-9]', '', 'g'), '')::integer
