SELECT evidence_details.*,ems_evidence_types.name,designation_master.designation_name as designation,unit_master.unit_name as posting,emp_master.emp_firstname ||' '||emp_master.emp_middlename ||' '||emp_master.emp_lastname as io_fname,evidence_details.type
						        from evidence_details
						        inner join ems_evidence_types on ems_evidence_types.id=evidence_type
						        inner join designation_master on designation_master.desig_master_id=io_designation
						        inner join unit_master on unit_master.id=io_ps
						        inner join emp_master on emp_master.id=cast(io_name as integer)
						        where fk_ems_case_id=13426 and fk_ems_panchnama_id=19655 and evidence_details.status=1


