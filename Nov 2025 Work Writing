SELECT 
    id, 
    act_type, 
    short_name,
    equivalent_act,
    equivalent_section,
    CONCAT_WS(' | ', short_name, equivalent_act, equivalent_section) AS act_section_mapped
FROM 
    ems_act_sections WHERE id = 1;
 
