    15-09-2025

    CREATE TABLE translations (
    id SERIAL PRIMARY KEY,             
    category VARCHAR(100) NOT NULL,    
    message_key VARCHAR(150) NOT NULL, 
    language VARCHAR(10) NOT NULL,      
    message TEXT NOT NULL               
   );


id	category	message_key	language	message
1	label	welcome_text	en	Welcome to my website
2	label	welcome_text	hi	मेरी वेबसाइट पर आपका स्वागत है
3	button	login_button	en	Login
4	button	login_button	hi	लॉगिन


CREATE TABLE translations (
    id INT GENERATED ALWAYS AS IDENTITY PRIMARY KEY,
    page VARCHAR(100) NOT NULL,         -- e.g., "login", "dashboard"
    category VARCHAR(100) NOT NULL,     -- e.g., "label", "button", "error"
    message_key VARCHAR(150) NOT NULL,  -- e.g., "welcome_text"
    language VARCHAR(10) NOT NULL,      -- e.g., "en", "hi"
    lang_text TEXT NOT NULL,            -- translated text
    CONSTRAINT unique_translation UNIQUE (page, category, message_key, language)
);


Original query
CREATE TABLE translations (
    id SERIAL PRIMARY KEY,  
    page VARCHAR(255) NULL,            
    category VARCHAR(255) NULL,    
    message_key VARCHAR(255) NULL,  
    language VARCHAR(10) NULL,      
    lang_text TEXT NULL,   
    created_at TIMESTAMP DEFAULT CURRENT_TIMESTAMP,
    updated_at TIMESTAMP DEFAULT CURRENT_TIMESTAMP           
);

ALTER TABLE translations RENAME TO ui_translation;


