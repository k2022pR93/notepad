Google map download
https://chatgpt.com/c/68d522ce-aa84-8328-90d4-61f375c11b4d

<button id="downloadMapBtn" class="btn btn-primary" style="float:right;"> <i class="fa fa-download" aria-hidden="true"></i> </button>
<script src="https://cdnjs.cloudflare.com/ajax/libs/html2canvas/1.4.1/html2canvas.min.js"></script>
document.getElementById('downloadMapBtn').addEventListener('click', function() {
    const mapElement = document.getElementById('map');

    html2canvas(mapElement, {
        useCORS: true, // important if tiles come from a different domain
        allowTaint: true
    }).then(canvas => {
        // Convert canvas to image
        const link = document.createElement('a');
        link.download = `map_${new Date().toISOString().slice(0,19).replace(/:/g,'-')}.png`;
        link.href = canvas.toDataURL('image/png');
        link.click();
    }).catch(err => {
        console.error("Error capturing map:", err);
    });
});

