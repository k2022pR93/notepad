ALTER TABLE breath_analyzer_transactions
    ADD COLUMN model VARCHAR(225),
    ADD COLUMN test_mode VARCHAR(225),
    ADD COLUMN test_no VARCHAR(50),
    ADD COLUMN blank_test_result VARCHAR(225),
    ADD COLUMN coordinate_position VARCHAR(225),
    ADD COLUMN certified_single_number VARCHAR(225),
    ADD COLUMN sensor_element_number VARCHAR(225),
    ADD COLUMN officer_name VARCHAR(225),
    ADD COLUMN officer_badge_no VARCHAR(225),
    ADD COLUMN remarks_notes TEXT,
    ADD COLUMN driver_name VARCHAR(225),
    ADD COLUMN license_no VARCHAR(225),
    ADD COLUMN vehicle_number VARCHAR(225),
    ADD COLUMN verification_date DATE,
    ADD COLUMN calibration_date1 DATE,
    ADD COLUMN calibration_date2 DATE,
    ADD COLUMN calibration_date10 DATE,
    ADD COLUMN driver_signature TEXT,
    ADD COLUMN police_signature TEXT,
    ADD COLUMN ps_name VARCHAR(225);

    ALTER TABLE breath_analyzer_transactions
    ADD COLUMN officer_designation VARCHAR(225),
    ADD COLUMN officer_mobile_no VARCHAR(20);

    ALTER TABLE breath_analyzer_transactions
    ADD COLUMN address TEXT,
    ADD COLUMN sevarth_number VARCHAR(100);

