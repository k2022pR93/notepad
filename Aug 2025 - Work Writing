CREATE TABLE breath_analyzer_transactions (
    id SERIAL PRIMARY KEY,
    recordid VARCHAR(50),
    recordtype VARCHAR(20),
    deviceno VARCHAR(50),
    devicemacorimei VARCHAR(100),
    testtype VARCHAR(50),
    testdata VARCHAR(20),
    testunit INTEGER,
    testtime TIMESTAMP WITHOUT TIME ZONE,
    b2time TIMESTAMP WITHOUT TIME ZONE,
    b10time TIMESTAMP WITHOUT TIME ZONE,
    checktime TIMESTAMP WITHOUT TIME ZONE,
    validnum INTEGER,
    validinfo1 TEXT,
    validinfo2 TEXT,
    validinfo3 TEXT,
    validinfo4 TEXT,
    validinfo5 TEXT,
    validinfo6 TEXT,
    picflag BOOLEAN,
    sinfo VARCHAR(10),
    ewinfo VARCHAR(20),
    lng VARCHAR(20),
    lat VARCHAR(20),
    pok BOOLEAN,
    conformitynum VARCHAR(50),
    componentlos TEXT,
    photo_path VARCHAR(255),
    created_at TIMESTAMP WITHOUT TIME ZONE DEFAULT now()
);


